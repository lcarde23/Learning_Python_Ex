favorite_numbers = {
    'mandy': [42, 28, 9],
    'micah': [23],
    'gus': [7, 8],
    'hank': [1000000],
    'maggie': [0, 88, 20, 11],
    }
msg = 'favorite numbers are as follows:'
for name, numbers in favorite_numbers.items():
    print('\n' + name.title() + "'s " + msg)
    for number in numbers:
        print(str(number))
